# READ BEFORE GIT CLONE
# share/gen SSH keys
ssh-keygen -t ed25519 -C "<email>"
cat ~/.ssh/id_ed25519.pub	# upload to github

# update remote port
ssh -T git@github.com
git remote set-url origin git@github.com:<gname>/python_lib.git
git remote set-url origin git@github.com:<gclass>/<pname>-<gname>.git

# authetication
git config --global user.email "you@example.com"
git config --global user.name "Your Name"

